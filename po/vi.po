#: src/mdiff.c:1191
#, c-format
msgid ", %d items\n"
msgstr ", %d mục\n"

#: src/mdiff.c:1269
#, c-format
msgid "Read summary: %d files, %d items\n"
msgstr "Tóm tắt đọc: %d tập tin, %d mục\n"

#: src/mdiff.c:1665
#, c-format
msgid "Sorting"
msgstr "Đang sắp xếp"

#: src/mdiff.c:1682
#, c-format
msgid ", clustering"
msgstr ", đang xếp nhóm"

#: src/mdiff.c:1850 src/mdiff.c:1913
#, c-format
msgid ", done\n"
msgstr ", hoàn tất\n"

#: src/mdiff.c:1900
#, c-format
msgid "Sorting members"
