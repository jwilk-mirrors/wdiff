#: src/mdiff.c:1932
#, c-format
msgid "Work summary: %d clusters, %d members\n"
msgstr "Tóm tắt làm việc: %d nhóm, %d bộ phạn\n"

#: src/mdiff.c:2312
#, c-format
msgid "Work summary: %d clusters, %d members, %d overlaps\n"
msgstr "Tóm tắt làm việc: %d nhóm, %d bộ phạn, %d lần chồng lấp\n"

#: src/mdiff.c:2367 src/wdiff.c:244
#, c-format
msgid "select a terminal through the TERM environment variable"
msgstr "chọn thiết bị cuối dùng biến môi trường TERM"

#: src/mdiff.c:2370 src/wdiff.c:247
#, c-format
msgid "could not access the termcap data base"
msgstr "không thể truy cập đến cơ sở dữ liệu termcap"

#: src/mdiff.c:2372 src/wdiff.c:249
#, c-format
msgid "terminal type `%s' is not defined"
msgstr "chưa xác định kiểu thiết bị cuối « %s »"

#: src/mdiff.c:3751 src/mdiff.c:3763 src/wdiff.c:1189 src/wdiff.c:1201
#, c-format
msgid "%s: %d words"
msgstr "%s: %d từ"

#: src/mdiff.c:3754 src/mdiff.c:3766 src/wdiff.c:1192 src/wdiff.c:1204
#, c-format
msgid "  %d %d%% common"
msgstr "  %d %d%% chung"

#: src/mdiff.c:3756 src/wdiff.c:1194
#, c-format
msgid "  %d %d%% deleted"
msgstr "  %d %d%% bị xoá"

#: src/mdiff.c:3758 src/mdiff.c:3770 src/wdiff.c:1196 src/wdiff.c:1208
#, c-format
msgid "  %d %d%% changed"
msgstr "  %d %d%% đã thay đổi"

#: src/mdiff.c:3768 src/wdiff.c:1206
#, c-format
msgid "  %d %d%% inserted"
msgstr "  %d %d%% đã chèn"

#: src/mdiff.c:3797 src/wdiff.c:1248 src/wdiff2.c:178
#, c-format
msgid "Try `%s --help' for more information.\n"
msgstr "Hãy thử chạy câu lệnh trợ giúp « %s --help » để xem thêm thông tin.\n"

#: src/mdiff.c:3801
msgid ""
"mdiff - Studies multiple files and searches for similar sequences, it then\n"
"produces possibly detailed lists of differences and similarities.\n"
msgstr ""
"mdiff — xử lý nhiều tập tin và tìm kiếm các chuỗi tương tự,\n"
"sau đó xuất danh sách có thể chi tiết chứa các mục khác biệt và tượng tự.\n"

#: src/mdiff.c:3806
#, c-format
msgid "Usage: %s [OPTION]... [FILE]...\n"
msgstr "Sử dụng: %s [TÙY_CHỌN]... [TẬP_TIN]...\n"

#: src/mdiff.c:3810
msgid ""
"\n"
"Operation modes:\n"
msgstr ""
"\n"
"Chế độ thao tác:\n"

#: src/mdiff.c:3811
msgid "  -h                     (ignored)\n"
msgstr "  -h                     (bị bỏ qua)\n"

#: src/mdiff.c:3812
msgid "  -v, --verbose          report a few statistics on stderr\n"
msgstr "  -v, --verbose          thông báo vài thống kê trên stderr\n"

#: src/mdiff.c:3813 src/unify.c:293
msgid "      --help             display this help then exit\n"
msgstr "      --help             hiển thị trợ giúp này, sau đó thoát\n"

#: src/mdiff.c:3814 src/unify.c:294
msgid "      --version          display program version then exit\n"
msgstr "      --version          hiển thị phiên bản đang chạy, sau đó thoát\n"

#: src/mdiff.c:3816
msgid ""
"\n"
"Formatting output:\n"
msgstr ""
"\n"
"Định dạng kết xuất:\n"

#: src/mdiff.c:3817
msgid "  -T, --initial-tab       produce TAB instead of initial space\n"
msgstr "  -T, --initial-tab       xuất cột tab thay cho khoảng cách đầu tiên\n"

#: src/mdiff.c:3818
msgid "  -l, --paginate          paginate output through `pr'\n"
msgstr "  -l, --paginate          dàn trang kết xuất qua `pr'\n"

#: src/mdiff.c:3819
msgid "  -S, --string[=STRING]   take note of another user STRING\n"
msgstr "  -S, --string[=CHUỖI]   lưu ý đến chuỗi của người dùng khác\n"

#: src/mdiff.c:3820
msgid ""
"  -V, --show-links        give file and line references in annotations\n"
msgstr ""
"  -V, --show-links        phụ chú cũng chứa tham chiếu đến tập tin và số thứ "
"tự dòng\n"

#: src/mdiff.c:3821
msgid "  -t, --expand-tabs       expand tabs to spaces in the output\n"
msgstr ""
"  -t, --expand-tabs       mở rộng cột tab ra các khoảng cách trong kết xuất\n"

#: src/mdiff.c:3824
msgid ""
"\n"
"Debugging:\n"
msgstr ""
"\n"
"Gỡ rối:\n"

#: src/mdiff.c:3825
msgid "  -0, --debugging   output many details about what is going on\n"
msgstr "  -0, --debugging   xuất thông tin chi tiết về tiến trình gỡ rối\n"

#: src/mdiff.c:3828
msgid ""
"\n"
"Word mode options:\n"
msgstr ""
"\n"
"Tuỳ chọn về chế độ từng từ :\n"

#: src/mdiff.c:3829 src/wdiff.c:1266 src/wdiff2.c:196
msgid "  -1, --no-deleted           inhibit output of deleted words\n"
msgstr "  -1, --no-deleted           thu hồi xuất từ bị xoá\n"

#: src/mdiff.c:3830 src/wdiff.c:1267 src/wdiff2.c:197
msgid "  -2, --no-inserted          inhibit output of inserted words\n"
msgstr "  -2, --no-inserted          thu hồi xuất từ bị chèn\n"

#: src/mdiff.c:3831 src/wdiff.c:1268 src/wdiff2.c:198
msgid "  -3, --no-common            inhibit output of common words\n"
msgstr "  -3, --no-common            thu hồi xuất từ chung\n"

#: src/mdiff.c:3832
msgid "  -A, --auto-pager           automatically calls a pager\n"
msgstr "  -A, --auto-pager           tự động gọi trình dàn trang\n"

#: src/mdiff.c:3833
msgid "  -k, --less-mode            variation of printer mode for \"less\"\n"
msgstr "  -k, --less-mode            biến dạng chế độ máy in cho \"less\"\n"

#: src/mdiff.c:3834
msgid "  -m, --avoid-wraps          do not extend fields through newlines\n"
msgstr ""
"  -m, --avoid-wraps          không nên kéo dài trường qua ký tự dòng mới\n"

#: src/mdiff.c:3835
msgid "  -o, --printer              overstrike as for printers\n"
msgstr "  -o, --printer              gạch đè lên chữ, như trên máy in\n"

#: src/mdiff.c:3836
msgid "  -z, --terminal             use termcap as for terminal displays\n"
msgstr ""
"  -z, --terminal             dùng termcap như trên trình bày thiết bị cuối\n"

#: src/mdiff.c:3837
msgid ""
"  -K, --no-init-term         like -z, but no termcap init/term strings\n"
msgstr ""
"  -K, --no-init-term         giống « -z », nhưng không có chuỗi kiểu termcap "
"init/term\n"

#: src/mdiff.c:3838
msgid "  -O, --item-regexp=REGEXP   compare items as defined by REGEXP\n"
msgstr ""
"  -O, --item-regexp=BTCQ   so sánh các mục tùy theo biểu thức chính quy này\n"

#: src/mdiff.c:3839
msgid "  -W, --word-mode            compare words instead of lines\n"
msgstr "  -W, --word-mode            so sánh theo từng từ thay cho toàn dòng\n"

#: src/mdiff.c:4024
msgid "With no FILE, or when FILE is -, read standard input.\n"
msgstr "Không có TẬP_TIN hoặc TẬP_TIN là « - » thì đọc đầu vào tiêu chuẩn.\n"

#: src/mdiff.c:4026 src/unify.c:298 src/wdiff.c:1284 src/wdiff2.c:216
msgid "Report bugs to <wdiff-bugs@gnu.org>.\n"
msgstr "Hãy thông báo lỗi cho địa chỉ <wdiff-bugs@gnu.org>.\n"

#: src/mdiff.c:4037
#, c-format
msgid "ignoring option %s (not implemented)"
msgstr "đang bỏ qua tuỳ chọn %s (chưa thực hiện)"

#: src/mdiff.c:4334
#, c-format
msgid "cannot use -z, termcap not available"
msgstr "không thể dùng tùy chọn « -z » vì termcap hiện thời không sẵn sàng"

#: src/mdiff.c:4373
#, c-format
msgid "word merging for two files only (so far)"
msgstr "đã hợp nhất các từ của chỉ hai tập tin (đến lúc này)"

