#: src/mdiff.c:3693 src/mdiff.c:3705 src/wdiff.c:1182 src/wdiff.c:1194
#, c-format
msgid "  %d %d%% changed"
msgstr "  %d %d%% đã thay đổi"

#: src/mdiff.c:3703 src/wdiff.c:1192
#, c-format
msgid "  %d %d%% inserted"
msgstr "  %d %d%% đã chèn"

#: src/mdiff.c:3732 src/wdiff.c:1233 src/wdiff2.c:172
#, c-format
msgid "Try `%s --help' for more information.\n"
