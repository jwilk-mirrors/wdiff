#: src/mdiff.c:3801
msgid ""
"mdiff - Studies multiple files and searches for similar sequences, it then\n"
"produces possibly detailed lists of differences and similarities.\n"
msgstr ""
"mdiff - Bestudeert meerdere bestanden en zoekt naar gelijkenissen,\n"
"en produceert dan een opsomming van verschillen en overeenkomsten.\n"

#: src/mdiff.c:3806
#, c-format
msgid "Usage: %s [OPTION]... [FILE]...\n"
msgstr "Gebruik:  %s [OPTIE]... BESTAND...\n"

#: src/mdiff.c:3810
msgid ""
"\n"
"Operation modes:\n"
msgstr ""
"\n"
"Werkingsmodi:\n"

#: src/mdiff.c:3811
msgid "  -h                     (ignored)\n"
msgstr "  -h                     (genegeerd)\n"

#: src/mdiff.c:3812
msgid "  -v, --verbose          report a few statistics on stderr\n"
msgstr "  -v, --verbose          enige statistieken uitvoeren naar stderr\n"

#: src/mdiff.c:3813 src/unify.c:293
msgid "      --help             display this help then exit\n"
msgstr "      --help             deze hulptekst tonen en stoppen\n"

#: src/mdiff.c:3814 src/unify.c:294
msgid "      --version          display program version then exit\n"
msgstr "      --version          programmaversie tonen en stoppen\n"

#: src/mdiff.c:3816
msgid ""
"\n"
"Formatting output:\n"
msgstr ""
"\n"
"Uitvoer opmaken:\n"

#: src/mdiff.c:3817
msgid "  -T, --initial-tab       produce TAB instead of initial space\n"
msgstr "  -T, --initial-tab       TAB aanmaken in plaats van leidende spatie\n"

#: src/mdiff.c:3818
msgid "  -l, --paginate          paginate output through `pr'\n"
msgstr "  -l, --paginate          uitvoer opmaken via `pr'\n"

#: src/mdiff.c:3819
msgid "  -S, --string[=STRING]   take note of another user STRING\n"
msgstr "  -S, --string[=STRING]   let op andere gebruiker STRING\n"

#: src/mdiff.c:3820
msgid ""
"  -V, --show-links        give file and line references in annotations\n"
msgstr ""
"  -V, --show-links        bestands- en regelverwijzigen in annotaties tonen\n"

#: src/mdiff.c:3821
msgid "  -t, --expand-tabs       expand tabs to spaces in the output\n"
msgstr "  -t, --expand-tabs       tabs omzetten naar spaties in de uitvoer\n"

#: src/mdiff.c:3824
msgid ""
"\n"
"Debugging:\n"
msgstr ""
"\n"
"Debugging:\n"

#: src/mdiff.c:3825
msgid "  -0, --debugging   output many details about what is going on\n"
msgstr "  -0, --debugging   details tonen over wat er gedaan wordt\n"

#: src/mdiff.c:3828
msgid ""
"\n"
"Word mode options:\n"
msgstr ""
"\n"
"Opties woordmodus:\n"

#: src/mdiff.c:3829 src/wdiff.c:1266 src/wdiff2.c:196
msgid "  -1, --no-deleted           inhibit output of deleted words\n"
msgstr ""
"  -1, --no-deleted           onderdruk uitvoer van verwijderde woorden\n"

#: src/mdiff.c:3830 src/wdiff.c:1267 src/wdiff2.c:197
msgid "  -2, --no-inserted          inhibit output of inserted words\n"
msgstr ""
"  -2, --no-inserted          onderdruk uitvoer van toegevoegde woorden\n"

#: src/mdiff.c:3831 src/wdiff.c:1268 src/wdiff2.c:198
msgid "  -3, --no-common            inhibit output of common words\n"
msgstr ""
"  -3, --no-common            onderdruk uitvoer van gemeenschappelijke "
"woorden\n"

#: src/mdiff.c:3832
msgid "  -A, --auto-pager           automatically calls a pager\n"
msgstr "  -A, --auto-pager           roept automatisch een opmaker aan\n"

#: src/mdiff.c:3833
msgid "  -k, --less-mode            variation of printer mode for \"less\"\n"
msgstr "  -k, --less-mode            variatie op printermodus voor \"less\"\n"

#: src/mdiff.c:3834
msgid "  -m, --avoid-wraps          do not extend fields through newlines\n"
msgstr ""
"  -m, --avoid-wraps          velden niet uitbreiden via nieuwe regeleinden\n"

#: src/mdiff.c:3835
msgid "  -o, --printer              overstrike as for printers\n"
msgstr "  -o, --printer              doorhaling als bij printers\n"

#: src/mdiff.c:3836
msgid "  -z, --terminal             use termcap as for terminal displays\n"
msgstr "  -z, --terminal             gebruik termcap zoals voor terminals\n"

#: src/mdiff.c:3837
msgid ""
"  -K, --no-init-term         like -z, but no termcap init/term strings\n"
msgstr ""
"  -K, --no-init-term         gelijk aan -z, maar zonder termcap's init/term-"
"strings\n"

#: src/mdiff.c:3838
msgid "  -O, --item-regexp=REGEXP   compare items as defined by REGEXP\n"
msgstr ""
"  -O, --item-regexp=REGEXP   vergelijk items zoals gedefineerd door REGEXP\n"

#: src/mdiff.c:3839
msgid "  -W, --word-mode            compare words instead of lines\n"
msgstr "  -W, --word-mode            vergelijk woorden in plaats van regels\n"

#: src/mdiff.c:4024
msgid "With no FILE, or when FILE is -, read standard input.\n"
msgstr ""
"Als BESTAND '-' is, of als er geen BESTAND gegeven is,\n"
"wordt standaardinvoer gelezen.\n"

#: src/mdiff.c:4026 src/unify.c:298 src/wdiff.c:1284 src/wdiff2.c:216
msgid "Report bugs to <wdiff-bugs@gnu.org>.\n"
msgstr ""
"Rapporteer gebreken in het programma aan <wdiff-bugs@gnu.org>;\n"
"meld fouten in de vertaling aan <vertaling@vrijschrift.org>.\n"

#: src/mdiff.c:4037
#, c-format
msgid "ignoring option %s (not implemented)"
msgstr "negeer optie %s (niet geïmplementeerd)"

#: src/mdiff.c:4334
#, c-format
msgid "cannot use -z, termcap not available"
msgstr "kan optie '-z' niet gebruiken; termcap is niet beschikbaar"

#: src/mdiff.c:4373
#, c-format
msgid "word merging for two files only (so far)"
msgstr ""
"samenvoegen op woordniveau werkt (voorlopig) alleen voor twee bestanden"

