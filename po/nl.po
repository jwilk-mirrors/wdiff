#: src/wdiff.c:1252
msgid "wdiff - Compares words in two files and report differences.\n"
msgstr ""
"wdiff - vergelijkt woorden in twee bestanden en rapporteert verschillen.\n"

#: src/wdiff.c:1256
#, c-format
msgid ""
"Usage: %s [OPTION]... FILE1 FILE2\n"
"       %s -d [OPTION]... [FILE]\n"
msgstr ""
"Gebruik:  %s [OPTIE]... BESTAND1 BESTAND2\n"
"       %s -d [OPTIE]... [BESTAND]\n"

#: src/wdiff.c:1261 src/wdiff2.c:191
msgid ""
"Mandatory arguments to long options are mandatory for short options too.\n"
msgstr ""
"Verplichte argumenten van lange opties zijn ook verplicht voor korte "
"opties.\n"

#: src/wdiff.c:1264 src/wdiff2.c:194
msgid "  -C, --copyright            display copyright then exit\n"
msgstr "  -C, --copyright            copyright tonen en stoppen\n"

#: src/wdiff.c:1265 src/wdiff2.c:195
msgid ""
"  -K, --no-init-term         like -t, but no termcap init/term strings\n"
msgstr ""
"  -K, --no-init-term         gelijk aan -t, maar zonder termcap's init/term-"
"strings\n"

#: src/wdiff.c:1269 src/wdiff2.c:199
msgid "  -a, --auto-pager           automatically calls a pager\n"
msgstr "  -a, --auto-pager           roept automatisch een opmaker aan\n"

#: src/wdiff.c:1270
msgid "  -d, --diff-input           use single unified diff as input\n"
msgstr "  -d, --diff-input           gebruik enkele unified-diff als invoer\n"

#: src/wdiff.c:1271 src/wdiff2.c:200
msgid "  -h, --help                 display this help then exit\n"
msgstr "  -h, --help                 deze hulptekst tonen en stoppen\n"

#: src/wdiff.c:1272 src/wdiff2.c:201
msgid "  -i, --ignore-case          fold character case while comparing\n"
msgstr "  -i, --ignore-case          hoofdletters negeren bij vergelijken\n"

#: src/wdiff.c:1273 src/wdiff2.c:202
msgid "  -l, --less-mode            variation of printer mode for \"less\"\n"
msgstr "  -l, --less-mode            variatie van printermodus voor \"less\"\n"

#: src/wdiff.c:1274 src/wdiff2.c:203
msgid "  -n, --avoid-wraps          do not extend fields through newlines\n"
msgstr ""
"  -n, --avoid-wraps          velden niet uitbreiden met nieuwe regeleinden\n"

#: src/wdiff.c:1275 src/wdiff2.c:204
msgid "  -p, --printer              overstrike as for printers\n"
msgstr "  -p, --printer              doorhalen zoals bij printers\n"

#: src/wdiff.c:1276 src/wdiff2.c:206
msgid ""
"  -s, --statistics           say how many words deleted, inserted etc.\n"
msgstr ""
"  -s, --statistics           meld hoeveel woorden zijn verwijderd, "
"toegevoegd enz.\n"

#: src/wdiff.c:1277 src/wdiff2.c:207
msgid "  -t, --terminal             use termcap as for terminal displays\n"
msgstr ""
"  -t, --terminal             termcap gebruiken zoals bij terminalschermen\n"

#: src/wdiff.c:1278 src/wdiff2.c:208
msgid "  -v, --version              display program version then exit\n"
msgstr "  -v, --version              programmaversie tonen en stoppen\n"

#: src/wdiff.c:1279 src/wdiff2.c:209
msgid ""
"  -w, --start-delete=STRING  string to mark beginning of delete region\n"
msgstr "  -w, --start-delete=STRING  start verwijdering markeren met STRING\n"

#: src/wdiff.c:1280 src/wdiff2.c:210
msgid "  -x, --end-delete=STRING    string to mark end of delete region\n"
msgstr "  -x, --end-delete=STRING    einde verwijdering markeren met STRING\n"

#: src/wdiff.c:1281 src/wdiff2.c:211
msgid ""
"  -y, --start-insert=STRING  string to mark beginning of insert region\n"
msgstr "  -y, --start-insert=STRING  start invoeging markeren met STRING\n"

#: src/wdiff.c:1282 src/wdiff2.c:212
msgid "  -z, --end-insert=STRING    string to mark end of insert region\n"
msgstr "  -z, --end-insert=STRING    einde invoeging markeren met STRING\n"

#: src/wdiff.c:1400
#, c-format
msgid "cannot use -t, termcap not available"
msgstr "kan optie '-t' niet gebruiken; termcap is niet beschikbaar"

