#, c-format
msgid "Usage: %s [OPTION]... [FILE]...\n"
msgstr "Uso: %s [OPZIONE]... [FILE]...\n"

#: src/mdiff.c:3770
msgid ""
"\n"
"Operation modes:\n"
msgstr ""
"\n"
"Modi di operazione:\n"

#: src/mdiff.c:3771
msgid "  -h                     (ignored)\n"
msgstr "  -h                     (ignorato)\n"

#: src/mdiff.c:3772
msgid "  -v, --verbose          report a few statistics on stderr\n"
msgstr "  -v, --verbose          riporta alcune statistiche su stderr\n"

